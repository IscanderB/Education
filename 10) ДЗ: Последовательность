using System;

namespace Study
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int startNumber = 5,
                finalNimber = 103,
                step = 7;

            // использую цикл FOR т.к у нас заданы конкретные границы цикла

            for (int i = startNumber; i < finalNimber + step; i += step)
            {
                Console.WriteLine(i);
            }
        }
    }
}
